CREATE TABLE fullinfo ( ID int NOT NULL, City varchar(250), State varchar(255), Lat decimal(8,3), Lon decimal(8,3), outcount smallint(3), incount smallint(3), outcity1 tinyint, outcity2 tinyint, outcity3 tinyint, outcity4 tinyint, outcity5 tinyint, outcity6 tinyint, outcity7 tinyint, outcity8 tinyint, incity1 tinyint, incity2 tinyint, incity3 tinyint, incity4 tinyint, incity5 tinyint, incity6 tinyint, incity7 tinyint, incity8 tinyint);